{
    "manifest_version": 3,
    "name": "YouTube Video Sorter Extension",
    "version": "1.0.0",
    
    "description": "A YouTube extension with additional video sorting options on channel videos",
    "icons": {
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
    },

    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/@*/videos"],
            "js": ["Content.js"]
        }
    ]
}